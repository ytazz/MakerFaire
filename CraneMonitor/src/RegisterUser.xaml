<Window x:Class="CraneMonitor.RegisterUser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CraneMonitor"
        mc:Ignorable="d"
        Title="Player Registration" Height="300" Width="550">
    <Grid KeyDown="Grid_KeyDown">
        <Button Content="Register" HorizontalAlignment="Left" Margin="100,215,0,0" VerticalAlignment="Top" Width="75" Click="ButtonOk_Click"/>
        <Button Content="Cancel" HorizontalAlignment="Left" Margin="350,215,0,0" VerticalAlignment="Top" Width="75" Click="ButtonCancel_Click"/>
        <TextBox Name="UserName" HorizontalAlignment="Left" Height="23" Margin="99,54,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="342" FontSize="18"/>
        <Label Content="Name" HorizontalAlignment="Left" Margin="36,50,0,0" VerticalAlignment="Top" FontSize="18"/>
        <Label Name="Message1" Content="Label1" HorizontalAlignment="Left" Margin="39,100,0,0" VerticalAlignment="Top" Width="482" FontSize="18"/>
        <Label Name="Message2" Content="Label2" HorizontalAlignment="Left" Margin="39,140,0,0" VerticalAlignment="Top" Width="482" FontSize="18"/>

    </Grid>
</Window>
